<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\InLocation" table="in_location">
    <unique-constraints>
      <unique-constraint name="UNIQ_1967F5972D5B0234" columns="city"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="city" type="string" column="city" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="user" target-entity="InUser" mapped-by="region" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
