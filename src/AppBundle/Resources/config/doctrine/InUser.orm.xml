<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\InUser" table="in_user">
    <unique-constraints>
      <unique-constraint name="UNIQ_A728E92D92FC23A8" columns="username_canonical"/>
      <unique-constraint name="UNIQ_A728E92DA0D96FBF" columns="email_canonical"/>
      <unique-constraint name="UNIQ_A728E92DC05FB297" columns="confirmation_token"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usernameCanonical" type="string" column="username_canonical" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailCanonical" type="string" column="email_canonical" length="180" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enabled" type="boolean" column="enabled" nullable="false"/>
    <field name="salt" type="string" column="salt" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="confirmationToken" type="string" column="confirmation_token" length="180" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordRequestedAt" type="datetime" column="password_requested_at" nullable="true"/>
    <field name="roles" type="array" column="roles" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bornYear" type="date" column="born_year" nullable="false"/>
    <field name="fullName" type="string" column="full_name" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="region" target-entity="InLocation" inversed-by="user" fetch="LAZY">
      <join-table name="in_user_location">
        <join-columns>
          <join-column name="user_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="region_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
